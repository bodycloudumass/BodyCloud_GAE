<?xml version="1.0" encoding="UTF-8"?>
<modality>
	<init-action>
		<uri>/group/umass-test-position/contributors</uri>
		<method>GET</method>
	</init-action>
	<action>
		<uri>/engine/workflow/position</uri>
		<method>POST</method>
		<parameter>
			<name>sourceUser</name>
			<reference xpath="//users/user" />
		</parameter>
		<parameter>
			<name>sourceGroup</name>
			<value>umass-test-position</value>
		</parameter>
		<repeat>false</repeat>
	</action>
	<outputSpecification>
		<column>
			<name>position</name>
			<type>DOUBLE</type>
		</column>
		<column>
			<name>ID</name>
			<type>DOUBLE</type>
		</column>
	</outputSpecification>
</modality>